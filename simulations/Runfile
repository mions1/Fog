all:  results/NoTimeShared-0.sca results/NoTimeShared-1.sca results/NoTimeShared-2.sca results/NoTimeShared-3.sca results/NoTimeShared-4.sca results/TimeShared-0.sca results/TimeShared-1.sca results/TimeShared-2.sca results/TimeShared-3.sca results/TimeShared-4.sca

results/NoTimeShared-0.sca: testServerTS.ini
	rm -f results/NoTimeShared-0.sca results/NoTimeShared-0.vec results/NoTimeShared-0.vci
	./run -u Cmdenv -c NoTimeShared -r 0 -f testServerTS.ini
	sleep 10
